    <offsetfrom>handbase</offsetfrom>
    <Translation>16 4.5 8.4</Translation>
	<RotationMat>-1 0 0 0 1 0 0 0 -1 </RotationMat>
    <Geom type="trimesh" modifiable="false">
      <data>models/ROBOTIQ_S/finger-prox-1.iv 1</data>
