  </Body>
  
  
  <Joint name="JF1-1" type="hinge" enable="false">
    <Body>Finger1-0</Body>
	<Body>Finger1-1</Body>
	<offsetfrom>Finger1-0</offsetfrom>
	<weight>3.846</weight>
	<!--<anchor>0.4 11.6 9</anchor>-->

	<limitsdeg>90 0</limitsdeg>
    <maxvel>2</maxvel>
    <resolution>2</resolution>
    <maxtorque>5.2</maxtorque>
  </Joint>
  
  <Joint name="JF1-2" type="hinge" enable="false">
